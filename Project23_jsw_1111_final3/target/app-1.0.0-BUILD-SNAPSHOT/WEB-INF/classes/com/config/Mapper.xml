<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Mapper">

    <select id="findAll" resultType="MapDTO">
        select DEPTNO, DNAME, LOC
        from DEPT
        order by DEPTNO
    </select>
</mapper>